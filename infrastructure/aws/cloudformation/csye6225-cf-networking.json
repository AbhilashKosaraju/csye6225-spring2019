            "VpcId" : { "Ref" : "STACKNAMEcsye6225vpc" },
            "CidrBlock" : { "Ref" : "myvpcSubnet3" },
            "AvailabilityZone" : "us-east-1c",
         
            "Tags" : [
               {
                  "Key" : "Name",
                  "Value": {
                     "Fn::Join": [
                     "",
                     [
                        {
                           "Ref": "AWS::StackName"
